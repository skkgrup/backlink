    $('.emply-text-sec h5').each(function(){
    	var cadena1 =$(this).text();
    	var cadena2 = cadena1.slice(0,85);
    	$(this).addClass("titulo_h5");
    	$(this).attr('title', cadena1);
    	if(cadena1.length>85){
    	$(this).html(cadena2+'...');
    	}else{
    	$(this).html(cadena2);
    	}
    });
    $( "h5" ).hover(
      function() {
    	var cadena1=$(this).attr('title');
    	var cadena2 =$(this).text();
    	if(cadena1.length>85){
        $( this ).html(cadena1);
        $( this ).attr('title',cadena2);
    	$(this).addClass("importanteH5");
    	$(this).removeClass("titulo_h5");
    	}
      }, function() {
        var cadena1=$(this).attr('title');
    	var cadena2 =$(this).text();
    	if(cadena1.length>85){
        $( this ).html(cadena1);
        $( this ).attr('title',cadena2);
    	$(this).removeClass('importanteH5');
    	$(this).addClass('titulo_h5');
    	}
      }
    ); 
    
    
    </script>
